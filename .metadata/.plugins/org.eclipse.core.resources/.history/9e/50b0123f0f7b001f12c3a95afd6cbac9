package com.hw3.run;

public class Person {
	private String id;
	private String pw;

	Person(String id, String pw) {
		this.id = id;
		this.pw = pw;
	}

	void setId(String id) {
		this.id = id;
	}

	void setPw(String pw) {
		this.pw = pw;
	}
	
	String getId() {
		return id;
	}
	
	String getPw() {
		return pw;
	}
	
	public String toString() {
		return id + ":" + pw;
	}

}
