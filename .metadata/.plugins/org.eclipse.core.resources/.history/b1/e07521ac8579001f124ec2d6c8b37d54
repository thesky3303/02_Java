package com.hw2.model.dto;

public class Animal  {

	protected String name;
	
	public Animal(String string, String string2) {}

	public Animal(String name) {
		super();
		this.name = name;
	}

	public  void sound();

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
	
}
