package com.hw1.model.dto;

import java.util.ArrayList;
import java.util.List;

public class Friend {
	
	private String name;

	private List<Friend> FriendList = new ArrayList<Friend>();
	
	public Friend() {
		FriendList.add(new Friend("짱구"));
		FriendList.add(new Friend("철수"));
		FriendList.add(new Friend("유리"));
		FriendList.add(new Friend("훈이"));
		FriendList.add(new Friend("맹구"));
		
	}

	public Friend(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public Friend(String name, List<Friend> friendList) {
		super();
		this.name = name;
		FriendList = friendList;
	}

	public void setName(String name) {
		this.name = name;
	}
	

	public void pickLeader() {
		System.out.println(FriendList.get(0));
		
		
		
		System.out.println((Friend)FriendList.getName() );
	
	}
	
	
}
