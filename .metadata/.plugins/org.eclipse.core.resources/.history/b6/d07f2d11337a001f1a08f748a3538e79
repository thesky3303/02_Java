package com.hw3.model.service;

import java.util.Objects;

public class mate {

	private String mate;
	private String name2;

	public String getName2() {
		return name2;
	}

	public void setName2(String name2) {
		this.name2 = name2;
	}

	public mate() {
		super();
	}

	public mate(String mate) {
		super();
		this.mate = mate;
	}

	public mate(String mate, String name2) {
		super();
		this.mate = mate;
		this.name2 = name2;
	}

	@Override
	public int hashCode() {
		return Objects.hash(mate, name2);
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		mate other = (mate) obj;
		return Objects.equals(mate, other.mate) && Objects.equals(name2, other.name2);
	}

	public String getMate() {
		return mate;
	}

	public void setMate(String mate) {
		this.mate = mate;
	}

	@Override
	public String toString() {
		return "재료=" + mate;
	}
	
	

}
