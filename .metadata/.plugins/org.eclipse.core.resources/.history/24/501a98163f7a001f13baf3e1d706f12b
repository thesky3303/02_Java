package com.hw3.model.service;

import java.util.HashMap;
import java.util.Map;

public class mate {
	

	public String mate;
	public mate() {
		Map<Integer, String> mate = new HashMap<Integer, String>();
		mate.put(1, "면직물");
		mate.put(2, "플라스틱");
		mate.put(3, "유리");
		mate.put(4, "고무");
	}
	public String getMate() {
		return mate;
	}
	public void setMate(String mate) {
		this.mate = mate;
	}


}
